<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="150.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/16"
            fx:controller="app.controller.ConfigureTransformationSortController">
    <VBox layoutX="117.0" layoutY="14.0" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0"
          AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <AnchorPane VBox.vgrow="ALWAYS">
            <padding>
                <Insets left="10.0" right="10.0" top="10.0"/>
            </padding>
            <VBox layoutX="49.0" layoutY="-58.0" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0"
                  AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                <AnchorPane prefHeight="40.0" VBox.vgrow="NEVER">
                    <Label layoutX="73.0" layoutY="18.0" text="Choose the column to sort" AnchorPane.bottomAnchor="0.0"
                           AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                        <padding>
                            <Insets left="5.0"/>
                        </padding>
                    </Label>
                </AnchorPane>
                <AnchorPane VBox.vgrow="ALWAYS">
                    <GridPane layoutX="41.0" layoutY="8.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                        <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" maxWidth="134.0" minWidth="10.0" prefWidth="70.0"/>
                            <ColumnConstraints hgrow="SOMETIMES" maxWidth="229.0" minWidth="10.0" prefWidth="220.0"/>
                            <ColumnConstraints hgrow="SOMETIMES" maxWidth="203.0" minWidth="10.0" prefWidth="89.0"/>
                        </columnConstraints>
                        <rowConstraints>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        </rowConstraints>
                        <AnchorPane>
                            <Label alignment="CENTER" layoutY="20.0" text="Sort by" AnchorPane.leftAnchor="0.0"
                                   AnchorPane.rightAnchor="0.0">
                                <padding>
                                    <Insets left="5.0" right="10.0"/>
                                </padding>
                            </Label>
                        </AnchorPane>
                        <AnchorPane GridPane.columnIndex="1">
                            <ComboBox fx:id="column" layoutY="16.0" prefWidth="150.0" AnchorPane.leftAnchor="0.0"
                                      AnchorPane.rightAnchor="0.0"/>
                            <padding>
                                <Insets right="10.0"/>
                            </padding>
                        </AnchorPane>
                        <AnchorPane GridPane.columnIndex="2">
                            <ComboBox fx:id="order" layoutX="-18.0" layoutY="16.0" prefWidth="150.0"
                                      AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"/>
                        </AnchorPane>
                    </GridPane>
                </AnchorPane>
            </VBox>
        </AnchorPane>
        <AnchorPane prefHeight="40.0">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
            <HBox alignment="CENTER_RIGHT" layoutX="14.0" layoutY="-50.0" prefHeight="100.0" prefWidth="200.0"
                  AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                  AnchorPane.topAnchor="0.0">
                <Button fx:id="cancel" mnemonicParsing="false" prefWidth="80.0" text="Cancel"/>
                <Region prefWidth="10.0"/>
                <Button fx:id="ok" mnemonicParsing="false" prefWidth="80.0" text="OK"/>
            </HBox>
        </AnchorPane>
    </VBox>
</AnchorPane>
